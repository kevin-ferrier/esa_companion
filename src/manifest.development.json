{
  "manifest_version": 3,
  "name": "ESAMarathon Summer 2022 companion",
  "description": "Track the schedule of ESAMarathon summer 2022",
  "version": "1.1.2",
  "background": {
    "service_worker": "/background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "esa16.png",
      "32": "esa32.png",
      "48": "esa48.png",
      "128": "esa128.png",
      "256": "esa256.png"
    }
  },
  "permissions": ["alarms", "notifications", "storage"]
}
